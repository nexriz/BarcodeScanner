{
  "name": "My Extension",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_scripts" : [
      {
        "matches": [ "<all_urls>" ],
        "js": ["/static/js/index.js", "/scripts/content.js"]
      }
  ],
  "background": {
    "scripts": [
      "/scripts/socket.io-client.js", 
      "/scripts/background.js"
    ]
  },
  "permissions": [
    "tabs"
  ]
}
